#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
  int count=0;
   for(int n=100;n<=1000;n++){
    
     for(int i=1;i<=n;i++){
       if(n%i==0){
           count++;
       }
     }
    if(count==2){
        cout<<n<<" is an prime number"<<endl;
    }
    count=0;
  }
    return 0;
}
